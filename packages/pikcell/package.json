{
  "name": "@moxijs/pikcell",
  "version": "0.1.1",
  "description": "Pixel-perfect sprite editor built on MoxiJS and PIXI.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "build:clean": "npm run clean && npm run build",
    "watch": "webpack --mode development --watch",
    "clean": "rimraf dist node_modules/.cache",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
  "keywords": [
    "pikcell",
    "sprite-editor",
    "pixel-art",
    "moxijs",
    "moxi",
    "pixijs",
    "pico-8",
    "tic-80"
  ],
  "private": true,
  "author": "Chad Neff <ccneff@gmail.com> (https://github.com/ineffably)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ineffably/moxijs.git",
    "directory": "packages/pikcell"
  },
  "type": "commonjs",
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@types/jest": "^30.0.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^30.0.0",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "@moxijs/core": "file:../core",
    "@moxijs/ui": "file:../moxi-ui"
  }
}
