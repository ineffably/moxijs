{
  "name": "moxi",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "workspaces": [
    "packages/moxi",
    "packages/moxi-examples"
  ],
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "build:clean": "npm run build:clean --workspaces --if-present",
    "clean": "npm run clean --workspaces --if-present",
    "build:dev": "npm run build:dev --workspaces --if-present",
    "watch": "concurrently \"npm run watch --workspace=moxi-examples\" \"npm run watch --workspace=moxi\"",
    "watch:moxi": "npm run watch --workspace=moxi",
    "examples": "npm run local --workspace=moxi-examples",
    "test": "npm run test --workspace=moxi",
    "test:watch": "npm run test:watch --workspace=moxi",
    "test:ci": "npm run test:ci --workspace=moxi"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "rimraf": "^6.1.0",
    "typescript-eslint": "^8.46.3"
  }
}
