{
  "name": "moxijs-examples",
  "version": "1.0.0",
  "description": "Interactive examples for the MoxiJS game framework",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "build:clean": "npm run clean && npm run build",
    "watch": "webpack --mode development --watch",
    "clean": "rimraf dist node_modules/.cache",
    "local": "npm run dev",
    "test:ui": "playwright test --ui",
    "test:screenshots": "playwright test tests/ui-screenshots.spec.ts",
    "test:headed": "playwright test --headed"
  },
  "keywords": [
    "moxijs",
    "moxi",
    "game-engine",
    "pixi",
    "pixijs",
    "examples",
    "demos"
  ],
  "author": "Chad Neff <ccneff@gmail.com> (https://github.com/ineffably)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ineffably/moxijs.git",
    "directory": "packages/moxijs-examples"
  },
  "bugs": {
    "url": "https://github.com/ineffably/moxijs/issues"
  },
  "homepage": "https://github.com/ineffably/moxijs#readme",
  "type": "commonjs",
  "devDependencies": {
    "html-webpack-plugin": "^5.6.3",
    "playwright": "^1.56.1",
    "raw-loader": "^4.0.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/theme-one-dark": "^6.1.3",
    "codemirror": "^6.0.2",
    "@moxijs/core": "file:../core",
    "@moxijs/mini-gui": "file:../mini-gui",
    "pikcell": "file:../pikcell",
    "pixi.js": "^8.14.0"
  }
}
